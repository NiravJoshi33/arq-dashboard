{
	"name": "arq-dashboard",
	"version": "1.0.0",
	"description": "Real-time monitoring dashboard for ARQ task queues",
	"author": "Your Name",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/arq-dashboard.git"
	},
	"keywords": [
		"arq",
		"redis",
		"task-queue",
		"monitoring",
		"dashboard",
		"sveltekit"
	],
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.11",
		"@types/node": "^22.15.32",
		"@typescript-eslint/eslint-plugin": "^8.51.0",
		"@typescript-eslint/parser": "^8.51.0",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"bits-ui": "^1.4.6",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"echarts": "^5.6.0",
		"ioredis": "^5.6.1",
		"lucide-svelte": "^0.513.0",
		"mode-watcher": "^0.5.1",
		"pickleparser": "^0.2.1",
		"svelte-echarts": "^1.0.0",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^1.0.0"
	}
}
